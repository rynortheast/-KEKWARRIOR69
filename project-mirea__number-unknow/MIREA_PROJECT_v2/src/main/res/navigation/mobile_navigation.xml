<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.rynortheast.mirea_project_v2.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.rynortheast.mirea_project_v2.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.rynortheast.mirea_project_v2.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/nav_brouser"
        android:name="com.rynortheast.mirea_project_v2.navigation__browser"
        android:label="fragment_nav_brouser"
        tools:layout="@layout/fragment_navigation__browser" />
    <fragment
        android:id="@+id/navCalculator"
        android:name="com.rynortheast.mirea_project_v2.NavCalculator"
        android:label="fragment_nav_calculator"
        tools:layout="@layout/fragment_navigation__calculator" />
    <fragment
        android:id="@+id/navPlayer"
        android:name="com.rynortheast.mirea_project_v2.navigation__player"
        android:label="fragment_nav_player"
        tools:layout="@layout/fragment_navigation__player" />
</navigation>